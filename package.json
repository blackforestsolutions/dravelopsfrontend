{
  "name": "dravelopsfrontend",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "start": "ng serve --port 8094",
    "update:model": "graphql-codegen --config libs/efa/src/lib/domain/model/codegen.yml",
    "all:test": "nx run-many --target=test --all",
    "all:lint": "nx run-many --target=lint --all --max-warnings=0",
    "all:build": "nx run-many --target=build --all --configuration=production",
    "all:cover": "jest --coverage",
    "all:e2e": "npm run e2e:hvv",
    "e2e:hvv": "cypress run --env travelProvider=hvv",
    "e2e:dev": "cypress open --env travelProvider=hvv",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@apollo/client": "^3.4.7",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@nrwl/angular": "12.7.0",
    "apollo-angular": "^2.6.0",
    "graphql": "^15.5.1",
    "leaflet": "^1.7.1",
    "mat-timepicker": "^5.1.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@nrwl/cli": "12.7.0",
    "@nrwl/cypress": "12.7.0",
    "@nrwl/eslint-plugin-nx": "12.7.0",
    "@nrwl/jest": "12.7.0",
    "@nrwl/linter": "12.7.0",
    "@nrwl/tao": "12.7.0",
    "@nrwl/workspace": "12.7.0",
    "@types/jest": "26.0.24",
    "@types/leaflet": "^1.7.4",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "ng-mocks": "^12.4.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  }
